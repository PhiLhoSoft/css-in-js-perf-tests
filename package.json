{
  "name": "css-in-js-perf-tests",
  "version": "1.0.0",
  "description": "Performance tests for CSS-in-JS libraries",
  "scripts": {
    "compile": "rimraf lib/ && babel src/ --out-dir lib",
    "bench:simple": "node lib/simple-test",
    "bench:nested": "node lib/nested-test",
    "bench:style-overload": "node lib/style-overload-test",
    "bench:classes-overload": "node lib/classes-overload-test",
    "bench:complex": "node lib/complex-test",
    "bench": "npm run --silent compile && npm run --silent bench:simple && npm run --silent bench:nested && npm run --silent bench:style-overload && npm run --silent bench:classes-overload && npm run --silent bench:complex",
    "bundle": "babel-node src/size-test",
    "view:simple": "node lib/simple-test/view.js",
    "view:nested": "node lib/nested-test/view.js",
    "view:style-overload": "node lib/style-overload-test/view.js",
    "view:classes-overload": "node lib/classes-overload-test/view.js",
    "view:complex": "node lib/complex-test/view.js",
    "view": "rimraf output/ && npm run view:simple && npm run view:nested && npm run view:style-overload && npm run view:classes-overload && npm run view:complex",
    "lint": "eslint src/",
    "sc": "babel src/complex-test/ --out-dir lib/complex-test/ && node lib/complex-test/view.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PepijnSenders/css-in-js-perf-tests.git"
  },
  "keywords": [],
  "author": "Pepijn Senders <ps@hellofresh.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PepijnSenders/css-in-js-perf-tests/issues"
  },
  "homepage": "https://github.com/PepijnSenders/css-in-js-perf-tests",
  "dependencies": {
    "aphrodite": "^1.1.0",
    "babel-loader": "^6.2.7",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.2",
    "cssobj": "^1.0.1",
    "cssobj-core": "^1.0.0",
    "cssobj-plugin-gencss": "^2.0.3",
    "cssobj-plugin-localize": "^3.0.1",
    "cxs": "^2.1.0",
    "easy-table": "^1.0.0",
    "fela": "^4.1.0",
    "fela-plugin-fallback-value": "^4.1.1",
    "fela-plugin-prefixer": "^4.1.1",
    "free-style": "^2.0.0",
    "glamor": "^2.20.5",
    "gzip-size": "^3.0.0",
    "inline-style-prefixer": "^2.0.5",
    "j2c": "^0.11.2",
    "jss": "^6.1.0",
    "jss-cache": "^0.2.0",
    "jss-camel-case": "^3.0.0",
    "jss-global": "^0.3.0",
    "jss-nested": "^3.0.0",
    "jss-preset-default": "^1.1.0",
    "jss-vendor-prefixer": "^4.0.0",
    "rimraf": "^2.5.4",
    "styletron-client": "^2.3.0",
    "styletron-server": "^2.3.0",
    "styletron-utils": "^2.3.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-node6": "^11.0.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1"
  }
}
